<ul>
  <a href="/venues">Venues</a>
  <a href="/cities">Cities</a>
  <a href="/cities/<%= @city.id %>/venues"><%= @city.name%> Venues</a>
</ul>
<h3>City: <%= @city.name %>
  <%= button_to "Update #{@city.name}", "/cities/#{@city.id}/edit", method: :get %>
  <%= button_to "Delete City", "/cities/#{@city.id}", method: :delete %></h3>
<h3>Population: <%= @city.population %></h3>
<h3>Coastal: <%= @city.coastal %></h3>
<p>Number of venues: <%= @city.count_venues %></p>
